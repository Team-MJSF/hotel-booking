{
  "name": "hotel-booking-backend",
  "version": "1.0.0",
  "description": "Hotel Booking System Backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:generate": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "db:migrate": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "db:revert": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts",
    "db:seed": "cross-env NODE_ENV=development ts-node src/database/seeds/seeder.ts",
    "db:seed:test": "cross-env NODE_ENV=test ts-node src/database/seeds/seeder.ts",
    "db:seed:staging": "cross-env NODE_ENV=staging ts-node src/database/seeds/seeder.ts",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "db:fresh": "npm run db:revert && npm run db:migrate && npm run db:seed",
    "dev": "npm run start:dev",
    "dev:setup": "npm run build && npm run db:migrate && npm run db:seed",
    "dev:reset": "npm run build && npm run db:fresh",
    "dev:clean": "npm run build && npm run db:revert",
    "test:setup": "npm run build && npm run db:migrate && npm run db:seed:test",
    "test:reset": "npm run build && npm run db:fresh && npm run db:seed:test",
    "staging:setup": "npm run build && npm run db:migrate && npm run db:seed:staging",
    "staging:reset": "npm run build && npm run db:fresh && npm run db:seed:staging",
    "prestart:prod": "npm run build",
    "prestart:staging": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
